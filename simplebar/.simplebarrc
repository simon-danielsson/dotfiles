{
  "$schema": "https://raw.githubusercontent.com/Jean-Tinland/simple-bar/refs/heads/master/lib/schemas/config.json",
  "global": {
    "theme": "dark",
    "compactMode": false,
    "floatingBar": true,
    "noBarBg": true,
    "noColorInData": false,
    "bottomBar": false,
    "sideDecoration": false,
    "inlineSpacesOptions": false,
    "disableAnimations": true,
    "spacesBackgroundColorAsForeground": true,
    "widgetsBackgroundColorAsForeground": true,
    "widgetMaxWidth": "160px",
    "slidingAnimationPace": 4,
    "font": "Maple Mono NF, Monaco, Menlo, monospace",
    "fontSize": "11px",
    "yabaiPath": "$(which yabai)",
    "aerospacePath": "/opt/homebrew/bin/aerospace",
    "windowManager": "aerospace",
    "shell": "bash",
    "terminal": "Terminal",
    "disableNotifications": false,
    "enableMissives": false,
    "enableServer": true,
    "serverSocketPort": 7777,
    "yabaiServerRefresh": false,
    "aerospaceServerRefresh": true
  },
  "themes": {
    "lightTheme": "NightShiftLight",
    "darkTheme": "VscodeDarkModern",
    "colorMain": "#25252d",
    "colorMainAlt": "#25252d",
    "colorMinor": "#25252d",
    "colorAccent": "#9ec1a3",
    "colorRed": "#AAB3C0",
    "colorGreen": "#AAB3C0",
    "colorYellow": "#AAB3C0",
    "colorOrange": "#AAB3C0",
    "colorBlue": "#AAB3C0",
    "colorMagenta": "#AAB3C0",
    "colorCyan": "#AAB3C0",
    "colorBlack": "",
    "colorWhite": "#AAB3C0",
    "colorForeground": "#AAB3C0",
    "colorBackground": "#25252d"
  },
  "process": {
    "showOnDisplay": "",
    "displayForFocusedSpace": false,
    "displayOnlyCurrent": true,
    "centered": false,
    "showCurrentSpaceMode": false,
    "hideWindowTitle": false,
    "displayOnlyIcon": false,
    "expandAllProcesses": false,
    "displaySkhdMode": false,
    "displayStackIndex": false,
    "displayOnlyCurrentStack": false
  },
  "spacesDisplay": {
    "showOnDisplay": "",
    "exclusions": "",
    "titleExclusions": "",
    "spacesExclusions": "",
    "exclusionsAsRegex": false,
    "displayAllSpacesOnAllScreens": false,
    "hideDuplicateAppsInSpaces": false,
    "displayStickyWindowsSeparately": false,
    "hideCreateSpaceButton": false,
    "hideEmptySpaces": true,
    "showOptionsOnHover": false,
    "switchSpacesWithoutYabai": false,
    "customAeroSpaceDisplayIndexes": {}
  },
  "widgets": {
    "processWidget": true,
    "githubWidget": false,
    "weatherWidget": false,
    "netstatsWidget": false,
    "cpuWidget": true,
    "gpuWidget": false,
    "memoryWidget": true,
    "batteryWidget": true,
    "wifiWidget": true,
    "vpnWidget": false,
    "zoomWidget": true,
    "soundWidget": false,
    "micWidget": false,
    "dateWidget": true,
    "timeWidget": true,
    "keyboardWidget": false,
    "spotifyWidget": false,
    "cryptoWidget": false,
    "stockWidget": false,
    "youtubeMusicWidget": false,
    "musicWidget": false,
    "mpdWidget": false,
    "browserTrackWidget": false
  },
  "githubWidgetOptions": {
    "refreshFrequency": 600000,
    "showOnDisplay": "",
    "showIcon": true,
    "hideWhenNoNotification": false,
    "notificationUrl": "https://github.com/notifications",
    "ghBinaryPath": "/opt/homebrew/bin/gh"
  },
  "weatherWidgetOptions": {
    "refreshFrequency": 1800000,
    "showOnDisplay": "",
    "showIcon": true,
    "unit": "C",
    "hideLocation": false,
    "hideGradient": false,
    "customLocation": ""
